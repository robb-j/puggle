{
  "name": "puggle",
  "version": "0.5.5",
  "description": "A CLI for bootstrapping and keeping project config up-to-date",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "robb-j/puggle",
  "author": "Rob Anderson (https://r0b.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npx tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage",
    "preversion": "npm test -s && npm run readme-toc && npm run build && git add README.md",
    "readme-toc": "md-toc -i",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'",
    "start": "node dist/cli.js",
    "dev": "NODE_ENV=development node -r ts-node/register src/cli.ts"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "puggle": "./dist/cli.js"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "casex": "^3.0.0",
    "chalk": "^3.0.0",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.6",
    "got": "^9.6.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "merge": "^2.1.1",
    "prompts": "^2.3.2",
    "semver": "^7.3.2",
    "yaml": "^1.10.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.1",
    "@types/fs-extra": "^8.1.0",
    "@types/glob": "^7.1.3",
    "@types/got": "^9.6.11",
    "@types/jest": "^26.0.14",
    "@types/lodash.clone": "^4.5.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^14.11.2",
    "@types/prompts": "^2.0.9",
    "@types/semver": "^7.3.4",
    "@types/yaml": "^1.9.7",
    "@types/yargs": "^15.0.7",
    "coveralls": "^3.1.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "lint-staged": "^10.4.0",
    "nock": "^13.0.4",
    "prettier": "^2.1.2",
    "strip-ansi": "^6.0.0",
    "ts-jest": "^26.4.1",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testMatch": [
      "**/__tests__/**/*.spec.ts"
    ],
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
