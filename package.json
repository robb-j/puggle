{
  "name": "puggle",
  "version": "0.2.0-beta.7",
  "description": "A CLI for bootstrapping and keeping project config up-to-date",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "robb-j/puggle",
  "author": "Rob Anderson (https://r0b.io)",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "lint": "npx tsc --noEmit",
    "test": "jest",
    "coverage": "jest --coverage",
    "preversion": "npm test -s && npm run readme-toc && npm run build && git add README.md",
    "readme-toc": "md-toc -i",
    "prettier": "prettier --write '**/*.{js,ts,tsx,json,css,md}'",
    "start": "node dist/cli.js",
    "dev": "NODE_ENV=development node -r ts-node/register src/cli.ts"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "puggle": "./dist/cli.js"
  },
  "keywords": [],
  "engines": {
    "node": ">=8"
  },
  "dependencies": {
    "casex": "^3.0.0",
    "chalk": "^2.4.2",
    "fs-extra": "^8.1.0",
    "glob": "^7.1.4",
    "got": "^9.6.0",
    "lodash.clone": "^4.5.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "lodash.set": "^4.3.2",
    "merge": "^1.2.1",
    "prompts": "^2.1.0",
    "semver": "^6.3.0",
    "yaml": "^1.6.0",
    "yargs": "^13.2.4"
  },
  "devDependencies": {
    "@robb_j/md-toc": "^0.3.0",
    "@types/fs-extra": "^8.0.1",
    "@types/glob": "^7.1.1",
    "@types/got": "^9.6.8",
    "@types/jest": "^24.0.15",
    "@types/lodash.clone": "^4.5.6",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.merge": "^4.6.6",
    "@types/lodash.set": "^4.3.6",
    "@types/node": "^12.0.12",
    "@types/prompts": "^2.0.1",
    "@types/semver": "^6.2.0",
    "@types/yaml": "^1.0.2",
    "@types/yargs": "^13.0.0",
    "coveralls": "^3.0.4",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^9.0.2",
    "nock": "^11.7.0",
    "prettier": "1.18.2",
    "strip-ansi": "^5.2.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/dist/"
    ]
  }
}
